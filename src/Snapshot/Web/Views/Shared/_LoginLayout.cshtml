<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@RenderSection("page_title", required: true)</title>
    <link href="@(Html.BuildUrlFromExpression<CssController>(req => req.Index("site")))"
        rel="stylesheet" type="text/css" media="screen,projection" />
    <!--[if IE 7]>
    <link href="@(Html.BuildUrlFromExpression<CssController>(req => req.Index("ie")))"
        rel="stylesheet" type="text/css" media="screen, projection" />
    <![endif]-->
    <script src="@(Html.BuildUrlFromExpression<JavascriptController>(req => req.Index("frameworks")))"
        language="javascript" type="text/javascript"> </script>
</head>
<body>
    <script type="text/javascript">
    window.res = {
        viewport: 'SnapshotViewport'
    }
    Ext.onReady(function(){
        var viewport = Ext.create('Ext.container.Viewport', {
            id: res.viewport,
            renderTo:Ext.getBody(),
            layout: {
                align: 'center',
                type: 'vbox',
            }
        });

    });

    </script>
    @RenderBody()
</body>
</html>
