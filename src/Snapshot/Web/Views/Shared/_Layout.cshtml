<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@RenderSection("page_title", required:true)</title>
    <link href="@(Url.RouteUrl("Css",new { controller = "Css", action = "Index", group = "site" }))"
        rel="stylesheet" type="text/css" media="screen,projection" />
    <!--[if IE 7]>
    <link href="@(Url.RouteUrl("Css", new { controller = "Css", action = "Index", group = "ie" }))"
        rel="stylesheet" type="text/css" media="screen, projection" />
    <![endif]-->
    <!--[if IE 6]>
    <link href="@(Url.RouteUrl("Css",new { controller = "Css", action = "Index", group = "ie6" }))"
        rel="stylesheet" type="text/css" media="screen, projection" />
    <![endif]-->
    <link href="@(Url.RouteUrl("Css",new { controller = "Css", action = "Index", group = "print" }))" 
        rel="stylesheet" type="text/css" media="print" />
    <script src="@(Url.RouteUrl("Javascript",new { controller = "Javascript", action = "Index", group = "frameworks" }))"
        language="javascript" type="text/javascript"> </script>
    <script src="@(Url.RouteUrl("Javascript", new { controller = "Javascript", action = "Index", group = "site" }))"
        language="javascript" type="text/javascript"> </script>
</head>
<body>
    <div class="container_16">
        <div class="prefix_11 grid_5 current_user">
            @{Html.RenderPartial("_LogOnPartial");}

        </div>
        @RenderSection("menu", required: false)
        <div class="breadcrumbs">
            @RenderSection("breadcrumbs", required:false) 
        </div>
        <div class="title_bar">
            @RenderSection("title_bar", required: false)
        </div>
        <section id="main" class="grid_16">
            @RenderBody()
        </section>
        <section id="master_content_actions">
        
            <div class="grid_12">&nbsp;
                @RenderSection("master_content", required: false)
            </div>
            
            <div class="grid_4 noprint"> &nbsp;
                @RenderSection("master_actions", required: false)
            </div>
            
        </section>
        <section id="master_detail">
            <div class="grid_16">
            &nbsp;
            @RenderSection("master_detail", required: false)
            </div>
        </section>
        <footer class="grid_16">

        
        </footer>
        <div class="clear">
        </div>
    </div>
    @RenderSection("scripts", false)
</body>
</html>
