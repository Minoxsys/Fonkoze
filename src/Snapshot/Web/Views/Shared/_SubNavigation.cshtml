<script type="text/javascript">
    window.res.header.subnavigation.buttonTabs = {
        id: 'ButtonTabs',
        group: 'AdministrationButtonsGroup',
        outpost: 'OutpostAdministration',
        product: 'ProductAdministration',
        user: 'UserAdministration',
        campaign: 'CampaignAdministration',
        warehouse: 'WarehouseAdministration'
    };

    Ext.onReady(function () {
        var subNavRes = window.res.header.subnavigation;
        var subnav = Ext.getCmp(subNavRes.id);
        subnav.add([
            {
                xtype: 'container',
                id: subNavRes.buttonTabs.id,
                margin: 0,
                padding: 0,
                width: 1000,
                layout: {
                    align: 'stretch',
                    padding: 0,
                    type: 'vbox'
                },
                items: [
                    {
                        xtype: 'container',
                        height: 5,
                        items: [{ 
                            xtype: 'label', 
                            html: '&nbsp;' 
                        }]
                    },{
                        xtype: 'container',
                        layout: {
                            type: 'column'},
                        flex: 1,
                        items: [
                            {
                                xtype: 'button',
                                id: subNavRes.buttonTabs.outpost,
                                toggleGroup: subNavRes.buttonTabs.group,
                                enableToggle: true,
                                pressed: true,
                                height: 35,
                                icon:'@(Url.RouteUrl<AssetsController>(it => it.Shared("/img/Administration/SubMenu/LocationNotSelected.png"), Web.Bootstrap.Routes.AssetRoutesRegistrar.SHARED))',
                                iconAlign: 'left',
                                margin: '0 7 0 20',
                                padding: '0 0 0 10',
                                text: 'Locations',
                                ui: 'snapshot-submenu-btn',
                                handler:function(){
                                    setUnselectedImagesToButtons();
                                    navigateTo(res.link.administration); 
                                }
                            },
                            {
                                xtype: 'button',
                                id: subNavRes.buttonTabs.product,
                                toggleGroup: subNavRes.buttonTabs.group,
                                enableToggle: true,
                                margin: '0 7 0 0',
                                padding: '0 0 0 10',
                                height: 35,
                                icon:'@(Url.RouteUrl<AssetsController>(it => it.Shared("/img/Administration/SubMenu/ProductNotSelected.png"), Web.Bootstrap.Routes.AssetRoutesRegistrar.SHARED))',
                                iconAlign: 'left',
                                text: 'Products',
                                ui: 'snapshot-submenu-btn',
                                handler:function(){
                                    setUnselectedImagesToButtons();
                                    navigateTo(res.link.productGroup); 
                                }
                            },
                            {
                                xtype: 'button',
                                id: subNavRes.buttonTabs.campaign,
                                toggleGroup: subNavRes.buttonTabs.group,
                                enableToggle: true,
                                height: 35,
                                margin: '0 7 0 0',
                                padding: '0 0 0 10',
                                icon:'@(Url.RouteUrl<AssetsController>(it => it.Shared("/img/Administration/SubMenu/CampaignNotSelected.png"), Web.Bootstrap.Routes.AssetRoutesRegistrar.SHARED))',
                                iconAlign: 'left',
                                text: 'Campaigns',
                                ui: 'snapshot-submenu-btn',
                                handler:function(){
                                    setUnselectedImagesToButtons();
                                    navigateTo(res.link.campaignOverview); 
                                }
                            },
                            {
                                xtype: 'button',
                                id: subNavRes.buttonTabs.user,
                                toggleGroup: subNavRes.buttonTabs.group,
                                enableToggle: true,
                                height: 35,
                                margin: '0 7 0 0',
                                padding: '0 0 0 10',
                                icon:'@(Url.RouteUrl<AssetsController>(it => it.Shared("/img/Administration/SubMenu/UserNotSelected.png"), Web.Bootstrap.Routes.AssetRoutesRegistrar.SHARED))',
                                iconAlign: 'left',
                                text: 'Users',
                                ui: 'snapshot-submenu-btn',
                                handler:function(){
                                    setUnselectedImagesToButtons();
                                    navigateTo(res.link.usersOverview);
                                }
                            },
                            {
                                xtype: 'button',
                                id: subNavRes.buttonTabs.warehouse,
                                toggleGroup: subNavRes.buttonTabs.group,
                                enableToggle: true,
                                height: 35,
                                margin: '0 7 0 0',
                                padding: '0 0 0 10',
                                icon: '@(Url.RouteUrl<AssetsController>(it => it.Shared("/img/Administration/SubMenu/WarehouseGreenNotSelected.png"), Web.Bootstrap.Routes.AssetRoutesRegistrar.SHARED))',
                                iconAlign: 'left',
                                text: 'Warehouses',
                                ui: 'snapshot-submenu-btn',
                                handler: function() {
                                    setUnselectedImagesToButtons();
                                    navigateTo(res.link.warehouseadmin);
                                }
                            }
                        ]
                    }
                ]                
            }]);
                        
        subnav.ownerCt.doLayout();

        function setUnselectedImagesToButtons()
        {
            Ext.getCmp(subNavRes.buttonTabs.outpost).setIcon('@(Url.RouteUrl<AssetsController>(it => it.Shared("/img/Administration/SubMenu/LocationNotSelected.png"), Web.Bootstrap.Routes.AssetRoutesRegistrar.SHARED))');
            Ext.getCmp(subNavRes.buttonTabs.product).setIcon('@(Url.RouteUrl<AssetsController>(it => it.Shared("/img/Administration/SubMenu/ProductNotSelected.png"), Web.Bootstrap.Routes.AssetRoutesRegistrar.SHARED))');
            Ext.getCmp(subNavRes.buttonTabs.campaign).setIcon('@(Url.RouteUrl<AssetsController>(it => it.Shared("/img/Administration/SubMenu/CampaignNotSelected.png"), Web.Bootstrap.Routes.AssetRoutesRegistrar.SHARED))');
            Ext.getCmp(subNavRes.buttonTabs.user).setIcon('@(Url.RouteUrl<AssetsController>(it => it.Shared("/img/Administration/SubMenu/UserNotSelected.png"), Web.Bootstrap.Routes.AssetRoutesRegistrar.SHARED))');
            Ext.getCmp(subNavRes.buttonTabs.warehouse).setIcon('@(Url.RouteUrl<AssetsController>(it => it.Shared("/img/Administration/SubMenu/WarehouseGreenNotSelected.png"), Web.Bootstrap.Routes.AssetRoutesRegistrar.SHARED))');
        }
    });
</script>