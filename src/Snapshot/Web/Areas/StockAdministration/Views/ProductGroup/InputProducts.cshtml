@model Web.Areas.StockAdministration.Models.ProductGroup.ProductGroupOutputModel
<div class="row">
    @Html.ValidationSummary()
</div>
<label for="Name">
    Product Group Name
</label>
@Html.TextBoxFor(it => it.Name, new { maxlength = 50, autofocus = "autofocus", placeholder = "Product Group Name" })
<label for="Description">
    Product Group Description</label>
@Html.TextAreaFor(it => it.Description, new { maxlength = 250, autofocus = "autofocus", placeholder = "Product Group Description" })


