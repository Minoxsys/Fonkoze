@model Web.Areas.OutpostManagement.Models.Region.RegionOutputModel
@{Html.HiddenFor(m => m.Id);}
@section page_title{
Region : Edit
}
@section breadcrumbs{
    @Url.HomeLink("Dashboard") / @(Html.ActionLink<RegionController>(act => act.Overview(), "Regions Overview", OutpostManagementAreaRegistration.DEFAULT_ROUTE))
    > Edit Region : @Model.Name
}
@section master_content{

    @{Html.BeginForm();}
     <div style="display: none">
@Html.HiddenFor(m => m.Client.Id)
@Html.HiddenFor(m=>m.Client.Name)
@Html.HiddenFor(m=>m.Client.Created)
@Html.HiddenFor(m=>m.Client.Updated)

</div>
            <fieldset class="fieldsCollection ">
                @{
                    Html.RenderPartial("Input", Model);     
                }
            </fieldset>
            @Html.SubmitButton("submit-form", "Save") <span>or </span><a href="@(Html.BuildUrlFromExpression<RegionController>(it => it.Overview()))">
                    Cancel</a>
     
    @{Html.EndForm();}
}
