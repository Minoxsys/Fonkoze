@model Web.Areas.OutpostManagement.Models.Outpost.OutpostModelOutput
           

@{ var id = new Guid();}
<fieldset id="fieldsetInformation">
    <legend>Country Information</legend>

    <div class="row of-2">
       <div class="column">
            <label for="Name"> Name<text class="required-text">*</text> </label>
            @Html.TextBoxFor(m => m.Name, new { maxlength = 250, autofocus = "autofocus", placeholder = "Name" })
        </div>
        <div class="column">
            <label for="Type"> Type<text class="required-text">*</text> </label>
            @Html.TextBoxFor(m => m.Type, new { maxlength = 250, autofocus = "autofocus", placeholder = "Type" })
        </div>
        <div class="column">
            <label for="MobileNumber"> Mobile Number<text class="required-text">*</text> </label>
            @Html.TextBoxFor(m => m.MobileNumber, new { maxlength = 250, autofocus = "autofocus", placeholder = "Number" })
        </div>
        <div class="column">
            <label for="EmailAddress"> E-mail Address<text class="required-text">*</text> </label>
            @Html.TextBoxFor(m => m.Email, new { maxlength = 250, autofocus = "autofocus", placeholder = "Email" })
        </div>
    </div>

</fieldset>
